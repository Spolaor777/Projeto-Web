@model IEnumerable<SistemaSuporte.Models.RespostaIA>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Chamado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prompt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RespostaGerada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataGeracao)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Chamado.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prompt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RespostaGerada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataGeracao)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RespostaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RespostaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RespostaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
